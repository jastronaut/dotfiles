#!/bin/sh
#
# generate a random emoji

num="$(shuf -i1-33 -n1)"
case $num in
	1) echo "ヾ(・ω・*)ノ";;
	2) echo "(´∀｀)♡";;
	3) echo "Σ(・Д・)!?";;
	4) echo "(　ﾟДﾟ)＜!!";;
	5) echo "(•ө•)♡";;
	6) echo "(^・ω・^ )";;
	7) echo "ʕ•㉨•ʔ";;
	8) echo "[´・ω・\`]";;
	9) echo "(¬‿¬)";;
	10) echo "(ง •̀_•́)ง";;
	11) echo "(-, – )…zzzZZZ";;
	12) echo "√(≖‿≖√)";;
	13) echo "✲ﾟ｡.(✿╹◡╹)ﾉ☆.｡₀:*ﾟ✲ﾟ*:₀｡";;
	14) echo "(っ・ω・）っ≡≡≡≡≡≡☆)";;
	15) echo "(´・ω・\`)";;
	16) echo "(ง ˙o˙)ว";;
	17) echo "d (^‿^✿)";;
	18) echo "(•́⌄•́๑)✧";;
	19) echo "(｡♥‿♥｡)";;
	20) echo "(°◡°♡).:｡";;
	21) echo "(｡･ω･｡)ﾉ♡";;
	22) echo "♡＾▽＾♡";;
	23) echo "♡´･ᴗ･`♡`";;
	24) echo "( ˊᵕˋ  )♡.°";;
	25) echo "（〃・ω・〃）";;
	26) echo "( ˘ ³˘ )♥";;
	27) echo "(´ε｀ )♡";;
	28) echo "┐(‘～\`；)┌)";;
	29) echo "┐(ﾟ～ﾟ)┌";;
	30) echo "┐(￣ヮ￣)┌";;
	31) echo "╮(＾▽＾)╭";;
	32) echo "(´ ˙○˙ \`)";;
	33) echo "( ° ^ °  )";;
	*) echo "┐(‘～\`；)┌)";;
esac
